{
  "atomKeymap.promptV3Features": true,
  "breadcrumbs.enabled": false,

  "window.nativeTabs": true,
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "window.zoomLevel": 1,

  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "Ayu Mirage Bordered",
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.fontAliasing": "auto",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",

  "editor.autoIndent": "brackets",
  "editor.colorDecorators": false,
  "editor.cursorBlinking": "phase",
  "editor.cursorStyle": "block",
  "editor.detectIndentation": true,
  "editor.fontFamily": "SF Mono, JetBrains Mono, Monaco",
  "editor.fontSize": 16,
  "editor.lineHeight": 28,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.insertSpaces": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.scrollPredominantAxis": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    80,
    120
  ],
  "editor.suggestFontSize": 12,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.useTabStops": true,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\\\|;:'\\\",.<>/?",
  "editor.wrappingIndent": "indent",
  "editor.wordWrap": "on",
  "editor.snippetSuggestions": "bottom",
  "editor.bracketPairColorization.enabled": true,

  "security.workspace.trust.untrustedFiles": "open",

  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.autoUpdate": true,
  "extensions.ignoreRecommendations": true,

  "emmet.showSuggestionsAsSnippets": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },

  "workbench.colorCustomizations": {
    "statusBarItem.remoteBackground": "#5a34a0",
    "statusBarItem.remoteForeground": "#fff",
    "editorBracketHighlight.foreground6": "#E8E5E3",
    "editorBracketHighlight.foreground5": "#DFDCD8",
    "editorBracketHighlight.foreground4": "#D6D3CD",
    "editorBracketHighlight.foreground3": "#CCCAC2",
    "editorBracketHighlight.foreground2": "#B1AFA8",
    "editorBracketHighlight.foreground1": "#96948E",
    "editorBracketHighlight.foreground0": "#7A7974",
    "editorB5racketHighlight.unexpectedBracket.foreground": "#8C0F61",
  },

  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "node_modules": false,
  },
  "files.associations": {
    "*.jsx": "javascriptreact",
    "*.js": "javascriptreact"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/_build/**": true,
    "**/_opam/**": true,
    "**/esy.lock/**": true,
    "**/_esy/**": true,
    "**/lib/bs/**": true,
    "**/lib/js/**": true,
    "**/lib/ocaml/**": true,
    "**/*.bs.js": true,
    "**/rust/target/**": true
  },

  "remote.SSH.remoteServerListenOnSocket": true,
  "remote.SSH.configFile": "/Users/davesnx/.ssh/config",
  "remote.SSH.remotePlatform": {
    "febox1": "linux"
  },

  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
  },
  "gitlens.blame.separateLines": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": true,
  "gitlens.hovers.annotations.changes": false,
  "gitlens.hovers.annotations.enabled": false,
  "gitlens.hovers.avatars": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.keymap": "alternate",
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.views.repositories.includeWorkingTree": false,

  "tabnine.experimentalAutoImports": true,

  "search.exclude": {
    "**/.git": true,
    "**/.vscode": true,
    "**/.next": true,
    "**/build": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/_esy": true,
    "**/esy.lock": true,
    "**/_opam": true,
    "**/_build": true
  },

  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },

  "html.format.enable": true,
  "html.format.preserveNewLines": true,
  "html.format.indentInnerHtml": true,

  "javascript.updateImportsOnFileMove.enabled": "always",
  "js/ts.implicitProjectConfig.strictNullChecks": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.referencesCodeLens.enabled": false,
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "eslint.validate": [
    "javascript",
    "typescript",
  ],
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },

  "[reason]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "ocamllabs.ocaml-platform"
  },
  "[rescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "chenglou92.rescript-vscode"
  },
  "[ocaml]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ocamllabs.ocaml-platform"
  },

  "[json]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": [
      "document"
    ],
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
  },
  "[jsonc]": {
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": [
      "document"
    ],
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
  },
  "terminal.external.osxExec": "iTerm.app",
  "terminal.explorerKind": "external",
  "git.autofetch": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
}
