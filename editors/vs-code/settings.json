{
  /* Window */
  "window.nativeTabs": true,
  "window.titleBarStyle": "native",
  "window.zoomLevel": 2,
  "workbench.tree.indent": 15,
  "workbench.tree.renderIndentGuides": "always",
  "keyboard.dispatch": "keyCode",
  "window.title": "${dirty} ${rootName}${separator}${separator}${activeEditorShort}",
  "window.newWindowDimensions": "inherit",
  /* Workbench */
  "workbench.colorTheme": "Fosk",
  "workbench.activityBar.visible": false,
  "workbench.panel.opensMaximized": "never",
  "workbench.editor.labelFormat": "short",
  "workbench.settings.editor": "json",
  "workbench.panel.defaultLocation": "right",
  "workbench.colorCustomizations": {
    "[Fosk]": {
      "tree.indentGuidesStroke": "#707a8c",
      "editorBracketHighlight.foreground1": "#cbccc6",
      "editorBracketHighlight.foreground2": "#ffcc66",
      "editorBracketHighlight.foreground3": "#73d0ff",
      "editorBracketHighlight.foreground4": "#c172d9",
      "editorBracketHighlight.foreground5": "#bae67e",
      "editorBracketHighlight.foreground6": "#4fb1bc",
      "editorBracketHighlight.unexpectedBracket.foreground": "#ff3333"
    },
    "statusBarItem.remoteForeground": "#fff",
    "statusBarItem.remoteBackground": "#5a34a0"
  },
  "workbench.editor.untitled.hint": "hidden",
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.fontAliasing": "antialiased",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.tabSizing": "shrink",
  /* Explorer */
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 1,
  "explorer.confirmDelete": false,
  /* Files */
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**\/.esy": true
  },
  "files.watcherExclude": {
    "**\/.esy": false,
    "**\/_opam": false,
    "**\/.DS_Store": true,
    "node_modules": false,
    "**\/Thumbs.db": true,
    "**\/.git": false
  },
  "files.associations": {
    "*.jsx": "javascriptreact",
    "*.mly": "ocaml.menhir",
    "*.js": "javascriptreact"
  },
  /* Search */
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "search.exclude": {
    "**\/_build": true,
    "**\/node_modules": true,
    "**\/.git": true,
    "**\/dist": true,
    "**\/.vscode": true,
    "**\/_esy": true,
    "**\/build": true,
    "**\/esy.lock": true,
    "**\/_opam": true,
    "**\/.next": true
  },
  /* Breadcrumbs */
  "breadcrumbs.enabled": true,
  "breadcrumbs.icons": true,
  /* Editor */
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\\\|;:'\\\",.<>\/?",
  "editor.lineHeight": 1.9,
  "editor.bracketPairColorization.enabled": true,
  "editor.fontSize": 14,
  "editor.fontWeight": 400,
  "editor.wordWrap": "on",
  "editor.minimap.showSlider": "always",
  "editor.glyphMargin": false,
  "editor.detectIndentation": true,
  "editor.wrappingIndent": "indent",
  "editor.inlineSuggest.enabled": true,
  "editor.renderControlCharacters": true,
  "editor.useTabStops": true,
  "editor.snippetSuggestions": "bottom",
  "editor.cursorBlinking": "solid",
  "editor.insertSpaces": true,
  "editor.tabCompletion": "on",
  "editor.cursorStyle": "block",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnSave": false,
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "editor.formatOnSaveMode": "file",
  "editor.minimap.renderCharacters": false,
  "editor.tabSize": 2,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.fontFamily": "Lilex-Regular, LigaLexMono, IBM Plex Mono, SFMonoLigaturized-Regular, Fira Code, JetBrains Mono, Martian Mono, Monaco",
  "editor.codeLensFontSize": 14,
  "editor.codeLensFontFamily": "Lilex-Regular, LigaLexMono, IBM Plex Mono, SFMonoLigaturized-Regular, Fira Code, JetBrains Mono, Martian Mono, Monaco",
  "editor.fontLigatures": true,
  "editor.accessibilitySupport": "off",
  "editor.scrollPredominantAxis": true,
  "editor.rulers": [
    80,
    120
  ],
  "editor.unicodeHighlight.allowedCharacters": {
    "’": true,
    "‹": true,
    "›": true,
    "–": true
  },
  "editor.suggestFontSize": 14,
  "editor.suggest.insertMode": "replace",
  "editor.autoIndent": "brackets",
  "editor.colorDecorators": false,
  "editor.renderWhitespace": "boundary",
  "editor.linkedEditing": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.bracketPairsHorizontal": false,
  /* Terminal */
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.lineHeight": 1.3,
  "terminal.integrated.defaultLocation": "view",
  "terminal.integrated.fontWeight": "normal",
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.fontFamily": "Lilex-Regular, LigaLexMono",
  "terminal.integrated.allowMnemonics": true,
  "terminal.integrated.drawBoldTextInBrightColors": true,
  "terminal.explorerKind": "external",
  "terminal.integrated.env.osx": {},
  "security.workspace.trust.untrustedFiles": "open",
  /* Language specific */
  /* HTML */
  "html.format.enable": true,
  "html.format.preserveNewLines": true,
  "html.format.indentInnerHtml": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  /* TypeScript */
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.referencesCodeLens.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.inlayHints.parameterTypes.enabled": true,
  /* JavaScript */
  "js\/ts.implicitProjectConfig.strictNullChecks": true,
  "eslint.validate": [
    "javascriptreact",
    "javascript",
    "typescript"
  ],
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[javascriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "vs-code-prettier-eslint.prettierLast": true,
  /* OCaml */
  "ocaml.dune.autoDetect": true,
  "ocaml.server.codelens": true,
  "ocaml.server.extendedHover": true,
  "[dune]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ocamllabs.ocaml-platform"
  },
  "[ocaml]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ocamllabs.ocaml-platform"
  },
  "[ocaml.interface]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ocamllabs.ocaml-platform"
  },
  "[ocaml.opam]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ocamllabs.ocaml-platform"
  },
  /* ReScript */
  "[rescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "chenglou92.rescript-vscode"
  },
  /* JSON */
  "[json]": {
    "gitlens.codeLens.scopes": [
      "document"
    ],
    "editor.formatOnPaste": false,
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.formatOnSave": true,
    "editor.suggest.insertMode": "replace"
  },
  "[jsonc]": {
    "gitlens.codeLens.scopes": [
      "document"
    ],
    "editor.formatOnPaste": false,
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.formatOnSave": true,
    "editor.suggest.insertMode": "replace"
  },
  /* Reason */
  "[reason]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ocamllabs.ocaml-platform"
  },
  "[cram]": {
    "editor.tabSize": 2,
    "files.trimTrailingWhitespace": false,
  },
  /* Extensions */
  "extensions.autoUpdate": true,
  "extensions.ignoreRecommendations": true,
  /* GitLens */
  "gitlens.blame.separateLines": false,
  "gitlens.hovers.annotations.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.annotations.changes": false,
  "gitlens.advanced.repositorySearchDepth": 1,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.views.repositories.includeWorkingTree": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressGitVersionWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false
  },
  "gitlens.hovers.avatars": false,
  "gitlens.currentLine.enabled": true,
  "gitlens.keymap": "alternate",
  /* Copilot */
  "github.copilot.enable": {
    "markdown": true,
    "*": true,
    "plaintext": false,
    "yaml": false
  },
  /* Emmet */
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  /* Remote */
  "remote.SSH.remotePlatform": {
    "nspawn": "linux"
  },
  "remote.SSH.useLocalServer": false,
  "remote.SSH.remoteServerListenOnSocket": true,
  "remote.SSH.configFile": "\/Users\/davesnx\/.ssh\/config",
  /* Codesnap */
  "codesnap.showWindowControls": false,
  "codesnap.containerPadding": "5em",
  "codesnap.backgroundColor": "#525869",
  "codesnap.boxShadow": "rgba(0, 0, 0, 0.25) 0px 30px 50px",
  /* Apc Customize UI++ */
  "apc.activityBar": {
    "position": "bottom",
    "hideSettings": true,
    "size": 20
  },
  "apc.statusBar": {
    "height": 24,
    "fontSize": 10
  },
  "apc.electron": {
    "frame": false,
    "titleBarStyle": "hiddenInset",
    "trafficLightPosition": {
      "x": 8,
      "y": 10
    }
  },
  "apc.header": {
    "height": 34,
    "fontSize": 13
  },
  "apc.listRow": {
    "height": 23,
    "fontSize": 13
  },
  /* Other extensions */
  "where-am-i.color": "#FFFFFF",
  "material-icon-theme.folders.theme": "specific",
  "tabnine.experimentalAutoImports": true
}
