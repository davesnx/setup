{
  /* Window */
  "window.nativeTabs": true,
  "window.title": "${dirty} ${rootName}${separator}${separator}${activeEditorShort}",
  /* Workbench */
  "workbench.panel.opensMaximized": "never",
  "workbench.editor.labelFormat": "short",
  "workbench.panel.defaultLocation": "right",
  "workbench.colorTheme": "Ayu Mirage",
  "workbench.colorCustomizations": {
    "[Ayu Mirage][Ayu Mirage Bordered]": {
      "editorBracketHighlight.foreground1": "#96948E",
      "editorBracketHighlight.foreground2": "#B1AFA8",
      "editorBracketHighlight.foreground3": "#CCCAC2",
      "editorBracketHighlight.foreground4": "#D6D3CD",
      "editorBracketHighlight.foreground5": "#DFDCD8",
      "editorBracketHighlight.foreground6": "#E8E5E3"
    },
    "[Ayu Light][Ayu Light Bordered]": {
      "editorBracketHighlight.foreground6": "#2a2927",
      "editorBracketHighlight.foreground5": "#5d5b56",
      "editorBracketHighlight.foreground4": "#76746e",
      "editorBracketHighlight.foreground3": "#76746e",
      "editorBracketHighlight.foreground2": "#a6a49f",
      "editorBracketHighlight.foreground1": "#bebcb9"
    },
    "statusBarItem.remoteForeground": "#fff",
    "statusBarItem.remoteBackground": "#5a34a0",
  },
  "workbench.editor.untitled.hint": "hidden",
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.fontAliasing": "antialiased",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.tabSizing": "shrink",
  /* Keymap */
  "atomKeymap.promptV3Features": true,
  /* Explorer */
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "explorer.confirmDelete": false,
  /* Files */
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**\/.esy": true,
  },
  "files.watcherExclude": {
    "**\/.esy": false,
    "**\/_opam": false,
    "**\/.DS_Store": true,
    "node_modules": false,
    "**\/Thumbs.db": true,
    "**\/.git": false
  },
  "files.associations": {
    "*.jsx": "javascriptreact",
    "*.mly": "ocaml.menhir",
    "*.js": "javascriptreact"
  },
  /* Search */
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "search.exclude": {
    "**\/_build": true,
    "**\/node_modules": true,
    "**\/.git": true,
    "**\/dist": true,
    "**\/.vscode": true,
    "**\/_esy": true,
    "**\/build": true,
    "**\/esy.lock": true,
    "**\/_opam": true,
    "**\/.next": true
  },
  /* Editor */
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\\\|;:'\\\",.<>\/?",
  "editor.formatOnPaste": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.lineHeight": 1.9,
  "editor.bracketPairColorization.enabled": true,
  "editor.renderWhitespace": "boundary",
  "editor.fontSize": 14,
  "editor.fontWeight": 400,
  "editor.wordWrap": "on",
  "editor.minimap.showSlider": "always",
  "editor.detectIndentation": true,
  "editor.wrappingIndent": "indent",
  "editor.inlineSuggest.enabled": true,
  "editor.renderControlCharacters": true,
  "editor.useTabStops": true,
  "editor.snippetSuggestions": "bottom",
  "editor.cursorBlinking": "phase",
  "editor.insertSpaces": true,
  "editor.tabCompletion": "on",
  "editor.cursorStyle": "block",
  "editor.formatOnSave": false,
  "editor.minimap.renderCharacters": false,
  "editor.tabSize": 2,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.fontFamily": "SFMonoLigaturized-Regular, Fira Code, JetBrains Mono, Martian Mono, Monaco",
  "editor.fontLigatures": true,
  "editor.accessibilitySupport": "off",
  "editor.scrollPredominantAxis": true,
  "editor.rulers": [
    80,
    120
  ],
  "editor.unicodeHighlight.allowedCharacters": {
    "’": true,
    "‹": true,
    "›": true,
    "–": true
  },
  "editor.suggestFontSize": 14,
  "editor.autoIndent": "brackets",
  "editor.colorDecorators": false,
  /* Terminal */
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.lineHeight": 1.3,
  "terminal.integrated.defaultLocation": "view",
  "terminal.integrated.fontWeight": "normal",
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontSize": 18,
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.fontFamily": "SFMonoLigaturized-Regular",
  "terminal.integrated.allowMnemonics": true,
  "terminal.integrated.drawBoldTextInBrightColors": true,
  "terminal.explorerKind": "external",
  "terminal.integrated.env.osx": {},
  "settingsSync.keybindingsPerPlatform": false,
  "security.workspace.trust.untrustedFiles": "open",
  /* Language specific */
  /* HTML */
  "html.format.enable": true,
  "html.format.preserveNewLines": true,
  "html.format.indentInnerHtml": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  /* TypeScript */
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.referencesCodeLens.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.inlayHints.parameterTypes.enabled": true,
  /* JavaScript */
  "[javascriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "js\/ts.implicitProjectConfig.strictNullChecks": true,
  "eslint.validate": [
    "javascriptreact",
    "javascript",
    "typescript"
  ],
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  /* OCaml */
  "[dune]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ocamllabs.ocaml-platform"
  },
  "[ocaml]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ocamllabs.ocaml-platform"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  /* ReScript */
  "[rescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "chenglou92.rescript-vscode"
  },
  /* JSON */
  "[json]": {
    "gitlens.codeLens.scopes": [
      "document"
    ],
    "editor.formatOnPaste": false,
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.formatOnSave": true,
    "editor.suggest.insertMode": "replace"
  },
  "[jsonc]": {
    "gitlens.codeLens.scopes": [
      "document"
    ],
    "editor.formatOnPaste": false,
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.formatOnSave": true,
    "editor.suggest.insertMode": "replace"
  },
  /* Reason */
  "[reason]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "ocamllabs.ocaml-platform"
  },
  /* Extensions */
  "extensions.autoUpdate": true,
  "extensions.ignoreRecommendations": true,
  /* GitLens */
  "gitlens.blame.separateLines": false,
  "gitlens.hovers.annotations.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.hovers.annotations.changes": false,
  "gitlens.advanced.repositorySearchDepth": 1,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.views.repositories.includeWorkingTree": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressGitVersionWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false
  },
  "gitlens.hovers.avatars": false,
  "gitlens.currentLine.enabled": true,
  "gitlens.keymap": "alternate",
  /* Copilot */
  "github.copilot.enable": {
    "markdown": true,
    "*": true,
    "plaintext": false,
    "yaml": false
  },
  /* Emmet */
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  /* Remote */
  "remote.SSH.remotePlatform": {
    "nspawn": "linux"
  },
  "remote.SSH.remoteServerListenOnSocket": true,
  "remote.SSH.configFile": "\/Users\/davesnx\/.ssh\/config",
  /* Codesnap */
  "codesnap.showWindowControls": false,
  "codesnap.containerPadding": "5em",
  "codesnap.backgroundColor": "#525869",
  "codesnap.boxShadow": "rgba(0, 0, 0, 0.25) 0px 30px 50px",
  /* Other extensions */
  "where-am-i.color": "#FFFFFF",
  "material-icon-theme.folders.theme": "specific",
  "window.zoomLevel": 2,
  "settingsSync.ignoredExtensions": [
    "github.copilot"
  ],
}
